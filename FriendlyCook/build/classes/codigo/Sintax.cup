start with INICIO;

INICIO ::= OYE SANJI SENTENCIAS POR FAVOR | LISTO SANJI :s {: v.statusMarvin=false;  :}
;
               
SENTENCIAS ::= SENT_HACER_PEDIDO  :s {: v.audio.add(0);  :}|
               SENT_MOSTRAR_MENU  :s {: v.audio.add(1);  :} |  //1=Mostrar menu
               SENT_DAR_CUENTA    :s {: v.audio.add(2);  :} |
               SENT_DICTAR_MENU   :s {: v.audio.add(3);  :}
;

SENT_DAR_CUENTA ::= QUIERO LA CUENTA :s {: v.codObjPeticion.add("show,cuenta,mesa, ");  :}|
                    TRAER LA CUENTA  :s {: v.codObjPeticion.add("show,cuenta,mesa, ");  :} |
                    MOSTRAR LA CUENTA :s {: v.codObjPeticion.add("show,cuenta,mesa, ");  :}  
;   

SENT_MOSTRAR_MENU ::= QUIERO VER EL MENU  :s {: v.codObjPeticion.add("show,menu, , ");  :} | 
                      QUIERO VER LA CARTA  :s {: v.codObjPeticion.add("show,menu, , ");  :}| 
                      MOSTRAR EL MENU :s {: v.codObjPeticion.add("show,menu, , ");  :} | 
                      MOSTRAR LA CARTA  :s {: v.codObjPeticion.add("show,menu, , ");  :}
;

SENT_DICTAR_MENU ::= LEER EL MENU :s {: v.codObjPeticion.add("show,menu, , ");  :}  | 
                     LEER LA CARTA :s {: v.codObjPeticion.add("show,menu, , ");  :}

;

SENT_HACER_PEDIDO ::= QUIERO PLATILLO | 
                      SENT_HACER_PEDIDO QUIERO PLATILLO | 
                      SENT_HACER_PEDIDO Coma PLATILLO | 
                      SENT_HACER_PEDIDO PLATILLO |
                      SENT_HACER_PEDIDO Y PLATILLO 
;



